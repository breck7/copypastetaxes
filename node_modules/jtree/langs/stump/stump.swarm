   <div id="bam" class="someClass">hello world</div>
   
test uids
 arrange
  require ../../products/stump.nodejs.js
  constructWithParagraph
   div hello world
    id bam
    class someClass
 getFirstNode
  toHtmlWithSuids
   assertTypeIs string
   assertStringIncludes stumpUid
  isStumpNodeCheckbox
   assertStringIs false
  toHtml
   assertTypeIs string
  isInputType
   assertStringIs false
  getStumpNodeAttr id
   assertStringIs bam
test syntax
 arrange
  require ../../products/stump.nodejs.js
  constructWithParagraph
   div
    bern
     Hello world
 toCellTypeTree
  assertParagraphIs
   htmlTagNameCell
    bernKeywordCell
     anyHtmlContentCell anyHtmlContentCell