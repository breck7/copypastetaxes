#! /usr/bin/env node /usr/local/bin/jtree
test stumpBasics
 arrange
  require ../../products/stump.nodejs.js
  constructWithParagraph
   html
    id bam
    class someClass
    body
     div
      type foo
      bern
       Hello world
       This is some text
     span Hello more
 getAllErrors
  assertLengthIs 0
 toHtml
  assertParagraphIs
   <html id="bam" class="someClass">
    <body>
     <div type="foo">Hello world
   This is some text</div>
     <span>Hello more</span>
   </body>
   </html>
   
